{
  "name": "careunity-mobile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "e2e:build:ios": "detox build -c ios",
    "e2e:build:android": "detox build -c android",
    "e2e:test:ios": "detox test -c ios",
    "e2e:test:android": "detox test -c android",
    "prepare:deployment": "node scripts/prepare-deployment.js",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "publish:staging": "node scripts/prepare-deployment.js --env=staging && expo publish",
    "publish:production": "node scripts/prepare-deployment.js --env=production && expo publish",
    "update:translations": "node scripts/update-translations.js",
    "test:auth": "jest --testMatch='**/*auth*.test.tsx'",
    "test:components": "jest --testMatch='**/*components*/*.test.tsx'"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/netinfo": "^9.4.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "axios": "^1.6.2",
    "expo": "~49.0.15",
    "expo-speech": "~11.3.0",
    "expo-status-bar": "~1.6.0",
    "expo-updates": "~0.18.17",
    "i18next": "^23.7.6",
    "react": "18.2.0",
    "react-i18next": "^13.5.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-paper": "^5.11.1",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-vector-icons": "^10.0.2",
    "expo-notifications": "~0.20.1",
    "expo-device": "~5.4.0",
    "expo-constants": "~14.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "^0.72.6",
    "babel-jest": "^29.7.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "typescript": "^5.1.3"
  }
}
